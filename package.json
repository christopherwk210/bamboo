{
  "name": "bamboo",
  "version": "0.0.0",
  "description": "Cross-platform TinyPNG client üéç",
  "scripts": {
    "build.win": "npm run build.parcel && npm run build.electron.win32",
    "build.mac": "npm run build.parcel && npm run build.electron.mac",
    "build.electron.mac": "electron-packager ./electron --platform=darwin --out=./bin --ignore=/bin --asar --overwrite=true",
    "build.electron.win": "electron-packager ./src --platform=win32 --arch=x64 --out=./bin --ignore=/bin --asar --overwrite=true",
    "build.electron.win32": "electron-packager ./src --platform=win32 --arch=ia32 --out=./bin --ignore=/bin --asar --overwrite=true",
    "build.parcel": "npm run prepare && parcel build src/index.html --out-dir=electron/dist --public-url='./' --target electron",
    "clean": "node tools/clean.js",
    "create.package.json": "node tools/create-electron-package-file.js",
    "electron": "NODE_ENV=development electron electron/ --dev",
    "install.electron.dependencies": "npm --prefix ./electron install ./electron",
    "parcel": "parcel src/index.html --out-dir=tmp --target electron",
    "prepare": "npm run clean && npm run create.package.json && npm run install.electron.dependencies",
    "start": "npm run prepare && concurrently \"npm run parcel\" \"sleep 2 && npm run electron\" "
  },
  "keywords": [
    "tinypng",
    "electron",
    "image",
    "compression"
  ],
  "author": "Chris Anselmo <christopherwk210@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "electron-packager": "^12.0.0",
    "electron-settings": "^3.1.4",
    "feather-icons": "^4.7.0",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.7.0",
    "rimraf": "^2.6.2",
    "tinify": "^1.5.0",
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.0.0",
    "vue-template-compiler": "^2.5.16"
  }
}
